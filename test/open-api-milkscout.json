{
  "openapi": "3.0.1",
  "info": {
    "title": "MilkScout API",
    "description": "An API designed to allow creameries to trade dairy",
    "version": "1",
    "x-logo": {
      "url": "https://assets.milkscout-stage.eu/logo/logo.png",
      "altText": "MilkScout GmbH"
    }
  },
  "paths": {},
  "components": {
    "schemas": {
      "MilkProduct": {
        "type": "string",
        "enum": [
          "RAW_MILK",
          "CREAM_40",
          "SKIMMED_MILK",
          "CONCENTRATED_SKIMMED_MILK_35",
          "BUTTERMILK",
          "LACTOSE_CONCENTRATE_18",
          "FULL_CREAM_MILK_3_5",
          "LIQUID_WHEY",
          "GOAT_MILK_PAST",
          "SHEEP_MILK_PAST"
        ]
      },
      "OfferUpdateDto": {
        "required": [
          "amount",
          "id",
          "pickUpDate",
          "price",
          "product",
          "qualityFeatures"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "string",
            "format": "uuid"
          },
          "product": {
            "$ref": "#/components/schemas/MilkProduct"
          },
          "description": {
            "maxLength": 4096,
            "minLength": 0,
            "type": "string"
          },
          "amount": {
            "maximum": 32000,
            "minimum": 1000,
            "type": "integer",
            "format": "int32"
          },
          "pickUpDate": {
            "type": "string",
            "format": "date"
          },
          "price": {
            "maximum": 100,
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "correctionCost": {
            "minimum": 0,
            "type": "number",
            "format": "double"
          },
          "qualityFeatures": {
            "type": "array",
            "items": {
              "type": "string",
              "enum": [
                "CONV",
                "VLOG",
                "ORGANIC",
                "NATURLAND",
                "DEMETER",
                "BIOLAND",
                "EU_BIO",
                "HAY_MILK",
                "ALPINE_MILK",
                "MOUNTAIN_FARM_MILK",
                "PASTURE_MILK",
                "PRO_WEIDELAND",
                "KOSHER",
                "HALAL",
                "TSL_EINSTIEGSSTUFE",
                "TSL_PREMIUMSTUFE",
                "GLYPHOSATE_FREE",
                "DIRECTLY_FROM_FARMER",
                "IFS",
                "BRC",
                "FSSC_22000",
                "ISO_9001",
                "CERTIFICATE_RUSSIA",
                "CERTIFICATE_USA",
                "CERTIFICATE_CHINA",
                "ANIMAL_FEED_EU",
                "CONNECTIVITY"
              ]
            }
          }
        }
      },
      "ShippingDetailsRangeRequestDto": {
        "required": [
          "company",
          "destinationAddress",
          "destinationCompanyName",
          "sourceAddress",
          "sourceCompanyName"
        ],
        "type": "object",
        "properties": {
          "company": {
            "type": "string",
            "format": "uuid"
          },
          "sourceCompanyName": {
            "maxLength": 128,
            "minLength": 4,
            "type": "string"
          },
          "sourceAddress": {
            "$ref": "#/components/schemas/AddressDto"
          },
          "destinationCompanyName": {
            "maxLength": 128,
            "minLength": 4,
            "type": "string"
          },
          "destinationAddress": {
            "$ref": "#/components/schemas/AddressDto"
          }
        }
      },
      "AddressDto": {
        "required": [
          "city",
          "countryCode",
          "number",
          "state",
          "street",
          "zip"
        ],
        "type": "object",
        "properties": {
          "street": {
            "maxLength": 128,
            "minLength": 0,
            "type": "string"
          },
          "number": {
            "maxLength": 16,
            "minLength": 0,
            "type": "string"
          },
          "zip": {
            "maxLength": 32,
            "minLength": 0,
            "type": "string"
          },
          "city": {
            "maxLength": 128,
            "minLength": 0,
            "type": "string"
          },
          "state": {
            "maxLength": 128,
            "minLength": 0,
            "type": "string"
          },
          "countryCode": {
            "$ref": "#/components/schemas/CountryCode"
          }
        }
      },
      "CountryCode": {
        "type": "string",
        "enum": [
          "DE",
          "AT",
          "IT",
          "FR",
          "BE",
          "NL",
          "DK",
          "PL",
          "CZ",
          "CH",
          "ES",
          "SK",
          "HR",
          "RO",
          "SE",
          "GB",
          "RS",
          "AL",
          "LU",
          "ME",
          "MK",
          "EE",
          "LV",
          "LT",
          "BY",
          "RU",
          "BG",
          "UA"
        ]
      },
      
      "AdditionalProps": {
        "type": "object",
        "additionalProperties": {
          "type": "array",
          "items": {
            "type": "object",
            "properties": {
              "street": {
                "maxLength": 128,
                "minLength": 0,
                "type": "string"
              },
              "number": {
                "maxLength": 16,
                "minLength": 0,
                "type": "string"
              },
              "zip": {
                "maxLength": 32,
                "minLength": 0,
                "type": "string"
              }
            }
          }
        }
      }
    }
  }
}